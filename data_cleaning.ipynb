    {
      "cell_type": "code",
      "source": [
        "categorical_data = []\n",
        "numerical_data = []\n",
        "target = 'MIS_Status'\n",
        "for i in df1.drop(target, axis=1).columns:\n",
        "    if df1[i].dtype == 'object':\n",
        "        categorical_data.append(i)\n",
        "    else:\n",
        "        numerical_data.append(i)"
      ],
      "metadata": {
        "id": "PFo_E0-92yu6"
      },

      "execution_count": null,

