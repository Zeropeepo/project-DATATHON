      }
    },
    {
      "cell_type": "code",
      "source": [
        "X = df1.drop(target, axis=1)\n",
        "y = df1[target]"
      ],
      "metadata": {
        "id": "XzP_gH_sp5nA"
      },

      "execution_count": null,

      "execution_count": 80,

      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.3, random_state=42)"
      ],
      "metadata": {
        "id": "Z9uOYfvZom_5"
      },

      "execution_count": null,

      "execution_count": 81,

      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Create a CatBoost Pool\n",
        "train_pool = Pool(data=X_train, label=y_train, cat_features=categorical_data)\n",
        "test_pool = Pool(data=X_test, label=y_test, cat_features=categorical_data)\n",
        "\n",
        "# Initialize the CatBoostClassifier\n",
        "model = CatBoostClassifier(\n",
        "    iterations=100,\n",
        "    learning_rate=0.1,\n",
        "    depth=3,\n",
        "    loss_function='Logloss',\n",
        "    verbose=False  # Suppress output during training\n",
        ")\n",
        "\n",
        "# Train the model\n",
        "model.fit(train_pool)"
      ],
      "metadata": {
        "colab": {
